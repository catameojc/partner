<% content_for :banner_title do %>
  <%= fa_icon 'user lg' %>&nbsp; Create Partner
<% end %>
<%= simple_form_for @partner,  url: partners_path, method: :post do |f| %>
  <div class="panel panel-default panel-bg">
    <div class="panel-body">
     <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Partner Type
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.type %>
          <%= f.hidden_field :type, value: @partner.type %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Name
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.name %>
          <%= f.hidden_field :name, value: @partner.name %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Password
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.pwd %>
          <%= f.hidden_field :pwd, value: @partner.pwd %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Company Name
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.organization %>
          <%= f.hidden_field :organization, value: @partner.organization %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Website
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.url %>
          <%= f.hidden_field :url, value: @partner.url %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Nature of Business
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.nature %>
          <%= f.hidden_field :nature, value: @partner.nature %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Representative
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.representative %>
          <%= f.hidden_field :representative, value: @partner.representative %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Position
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.position %>
          <%= f.hidden_field :position, value: @partner.position %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Street
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.street %>
          <%= f.hidden_field :street, value: @partner.street %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        City
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.city %>
          <%= f.hidden_field :city, value: @partner.city %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        State/Province
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.state %>
          <%= f.hidden_field :state, value: @partner.state %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Postal Code
