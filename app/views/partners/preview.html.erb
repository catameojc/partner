        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.admin %>
          <%= f.hidden_field :admin, value: @partner.admin %>
        </div>
      </div>
      <p class="col-md-offset-3">
        <%= f.submit 'Create Partner', role: 'button', class: 'btn btn-primary btn-lg' %>
      </p>
  </div>
<% end %>
