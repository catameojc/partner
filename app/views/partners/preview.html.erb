        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Country
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.country_code %>
          <%= f.hidden_field :country_code, value: @partner.country_code %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Telephone
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.phone %>
          <%= f.hidden_field :phone, value: @partner.phone %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Fax
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.fax %>
          <%= f.hidden_field :fax, value: @partner.fax %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
        Email
        </label>
        <div class="col-sm-4 text-info" data-original-title="" title="">
          <%= @partner.email %>
          <%= f.hidden_field :email, value: @partner.email %>
        </div>
      </div>
      <div class="form-group string required disabled registrant_partner">
        <label class="string required control-label col-xs-12 col-sm-6 col-md-4 control-label" for="registrant_partner">
